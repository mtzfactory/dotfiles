{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "graphql",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "markdown",
    "typescript",
    "typescriptreact"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "eslint.packageManager": "yarn",
  "eslint.alwaysShowStatus": true,
  "eslint.run": "onSave",
  "typescript.preferences.importModuleSpecifier": "non-relative"
}
